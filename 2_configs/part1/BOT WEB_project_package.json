{
  "name": "magus-prime-x",
  "private": true,
  "version": "2.0.0",
  "description": "Magus Prime X - AI Powered Trading Bot",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "start:bot": "python bot.py",
    "electron-app": "electron electron-main.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "@tensorflow/tfjs": "^4.17.0",
    "axios": "^1.6.7",
    "chart.js": "^4.4.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "i18next": "^23.10.0",
    "lucide-react": "^0.344.0",
    "ml-matrix": "^6.11.0",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.65.1",
    "openai": "^4.28.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-speech-recognition": "^3.10.0",
    "socket.io-client": "^4.7.4",
    "talib-web": "^0.1.3",
    "technicalindicators": "^3.1.0",
    "tradingview-api": "^1.0.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node-telegram-bot-api": "^0.64.2",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-speech-recognition": "^3.9.5",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "chai": "^5.2.0",
    "concurrently": "^8.2.2",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vitest": "^1.3.1"
  },
  "build": {
    "appId": "com.magusprimex.trading",
    "productName": "Magus Prime X",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!**/*.pyc"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "assets/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}